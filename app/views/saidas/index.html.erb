<p id="notice"><%= notice %></p>

<h1>Saidas</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Valor</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saidas.each do |saida| %>
      <tr>
        <td><%= saida.data %></td>
        <td><%= saida.valor %></td>
        <td><%= saida.descricao %></td>
        <td><%= link_to 'Show', saida %></td>
        <td><%= link_to 'Edit', edit_saida_path(saida) %></td>
        <td><%= link_to 'Destroy', saida, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saida', new_saida_path %>
